.root {
  background-color: #7178ff;
  font-family: "Lato", sans-serif;
  font-size: 30px;
  width: 50px;
  height: 50px;
  display: inline-block;
  color: white;
  text-transform: uppercase;
  border-radius: 50%;
  cursor: pointer;
  user-select: none;
  outline: none;
}

.root:active {
  position: relative;
  top: 3px;
}
