.Navbar{
  z-index: 99;
  background-image: url("../../resource/images/header-nkar.png");
  background-repeat: no-repeat;
  background-size: cover;
}
.login{
  color: #fff!important;
  font-size: responsive 10px 16px;
  transition:all .5s;
  border:1px solid #fff;
  margin-right: 10px;
  padding: 1px 20px;
  font-weight:700;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  display: flex;
  justify-content: center;
  align-items: center;
  cursor: pointer;

}
.sign{
  color: #5f90dd!important;
  font-size: responsive 10px 16px;
  transition:all .5s;
  border:1px solid #fff;
  padding: 1px 15px;
  background: #fff;
  font-weight:700;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  display: flex;
  justify-content: center;
  align-items: center;
  cursor: pointer;

}
.sign:hover{
  background: transparent!important;
  color: #fff!important;
}
.login:hover,.loginModal:hover{
  background: #fff!important;
  color: #5f90dd!important;
}
.brend{
  color: #fff!important;
  font-size: responsive 26px 40px;
  display: flex;
  justify-content: start;
  align-items: center;
  font-family: 'Alice', serif;

}
@media(max-width: 575px){
  .login,.sign{
    text-align: center;
    background: rgba(255, 255, 255, 0.25);
    margin: 2px;
    font-size: 16px;
    color: #fff !important;

  }
  .sign:hover,.login:hover .link:hover{
    background: rgba(255, 255, 255, 0.49) !important;
    color: #5f90dd!important;
  }
  .loginModal,.signModal,.link{
    font-size: 16px !important;
  }
  .link{

    transition:all .5s;
    border:1px solid #fff;
    padding: 1px 15px;
    font-weight:700;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
    text-align: center;
    background: rgba(255, 255, 255, 0.25);
    margin: 2px !important;
    color: #fff !important;

  }
  .collapse{
    text-align: center !important;
  }

}

.sign_up{
  margin-right: 5px;
}
.modal{
  transition: opacity 300ms ease-in;
  opacity:1;
  width:100%;
  position: fixed;
  z-index:999;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  left:0;
}

.none{
  outline: 0;
  opacity: 0.01;
  transition: opacity 300ms ease-in;
  position:absolute;
  width:100%;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index:0;
  left:0;
  height: 100%;

}
.modalBackdropOne{
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1040;
  background-color: #000;
  opacity: 0.5;
}
li{
  list-style-type: none;
}
.modallog{
  color: #5f90dd;
  font-size: 20px;
  font-weight: 700;
  padding: 10px;
}
a, a:hover,a:focus,a:active{
  text-decoration: none;
  outline: 0;
}
.formControl:focus{
  border-color: rgba(95, 144, 221, 0.55);
}

.link{
  margin-right:25px;
  font-size: responsive 10px 16px;
  color: #fff;
  font-weight:700;
}
.icon{
  display: none;
}
@media(max-width: 575px){
  .icon{
    vertical-align: middle;
    color: #fff;
    font-size: 25px;
    float: left;
    display: block;
    position: absolute;
    right: 15px;
    top:15px;
  }

}
.brend>img{
  max-width: 30px;
  margin-right: 5px;
}
